<?xml version="1.0"?>
<!--
###################################################
##PLL Reference Out
###################################################
 -->
<block>
	<name>PLL Ref Out</name>
	<key>gr_pll_refout_cc</key>
	<import>from gnuradio import gr</import>
	<make>gr.pll_refout_cc($alpha, $beta, $max_freq, $min_freq)</make>
	<param>
		<name>Alpha</name>
		<key>alpha</key>
		<type>real</type>
	</param>
	<param>
		<name>Beta</name>
		<key>beta</key>
		<type>real</type>
	</param>
	<param>
		<name>Max Freq</name>
		<key>max_freq</key>
		<type>real</type>
	</param>
	<param>
		<name>Min Freq</name>
		<key>min_freq</key>
		<type>real</type>
	</param>
	<sink>
		<name>in</name>
		<type>complex</type>
	</sink>
	<source>
		<name>out</name>
		<type>complex</type>
	</source>
</block>
